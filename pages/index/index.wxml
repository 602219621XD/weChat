<!-- 分类头 -->
<view class="index_cates">
  <navigator class="index_navigator"
    wx:for="{{cateList}}"
    wx:key="id"
    url="../{{item.caption}}/index"
  >
  <image class="cate_img" mode="widthFix" src="http://182.92.120.91:1337{{item.url}}"/>
  <view class="cate_name">{{item.name}}</view>
</navigator>
</view>
