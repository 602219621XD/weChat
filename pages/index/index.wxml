<view>

   <!-- 轮播图头 -->
  <view> 
  <swiper class="swiperCss" autoplay indicator-dots circular indicator-color="brow" indicator-active-color="white">
    <swiper-item
      wx:for="{{swiperList}}"
      wx:key="id"
    >
      <navigator>
        <image class="swiperimg" mode="widthFix" src="http://182.92.120.91:1337{{item.url}}"/>
      </navigator>
    </swiper-item>
  </swiper>

  </view>
  <!-- 轮播图尾 -->
